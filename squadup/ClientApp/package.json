    "proxy": {
        "/api/": {
            "target": "http://localhost:5001"
        }
    },
    "dependencies": {
        "axios": "^0.18.0",
        "react": "^16.4.2",
        "react-dom": "^16.4.2",
        "react-redux": "^5.0.7",
